fun stringify(number: __builtin_i32) -> __builtin_str
{
    if number >= 10
        return stringify(number / 10) + digit(number % 10)
    return digit(number)
}

fun digit(number: __builtin_i32) -> __builtin_str
{
    if number == 9
        return "9"
    if number == 8
        return "8"
    if number == 7
        return "7"
    if number == 6
        return "6"
    if number == 5
        return "5"
    if number == 4
        return "4"
    if number == 3
        return "3"
    if number == 2
        return "2"
    if number == 1
        return "1"
    return "0"
}

fun main() -> __builtin_str
{
    return stringify(1337)
}
